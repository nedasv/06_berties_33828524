<!doctype html>
<html>
  <head>
    <title><%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Here are all the users:</h1>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.username %>: <%= user.first_name%> <%= user.last_name%> <%= user.email%></li>
        <% }) %>    
    </ul>
  </body>
</html>